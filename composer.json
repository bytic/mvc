{
  "name": "bytic/mvc",
  "description": "Php Framework",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "require": {
    "php": "^7.0|^8.0",
    "bytic/collections": "^1.0",
    "bytic/container": "^1.0|^2.0",
    "bytic/config": "^1.0",
    "bytic/inflector": "^1.0",
    "bytic/utility": "^1.0"
  },
  "require-dev": {
    "bytic/http": "^1.0",
    "phpunit/phpunit": "^7.2|^8.0|^9.0",
    "mockery/mockery": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Nip\\Mvc\\": "src/"
    },
    "classmap": [
      "_legacy",
      "src"
    ],
    "files": [
      "src/functions/common.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Mvc\\Tests\\": "tests/src",
      "Nip\\Mvc\\Fixtures\\": "tests/fixtures"
    }
  }
}
