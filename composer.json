    "bytic/container": "^1.0",
    "bytic/config": "^1.0",
    "bytic/utility": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2|^8.0|^9.0",
    "mockery/mockery": "^1.1"
  }
}
